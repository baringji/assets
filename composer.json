{
    "name": "inpsyde/assets",
    "description": "Package to manage assets in WordPress.",
    "type": "library",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Christian BrÃ¼ckner",
            "email": "chris@chrico.info",
            "homepage": "https://www.chrico.info",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "brain/monkey": "^2",
        "mikey179/vfsstream": "~1.6",
        "squizlabs/php_codesniffer": "3.2.*",
        "inpsyde/php-coding-standards": "0.*"
    },
    "autoload": {
        "psr-4": {
            "Inpsyde\\Assets\\": "src/"
        },
        "files": [
            "inc/functions.php",
            "inc/bootstrap.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Inpsyde\\Assets\\Tests\\": "tests/phpunit/"
        }
    },
    "config": {
        "optimize-autoloader": true
    }
}
